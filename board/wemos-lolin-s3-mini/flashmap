36k		0x009000	60k		0x00efff	24k		0x006000	nvs
60k		0x00f000	68k		0x010fff	8k		0x002000	otadata
68k		0x011000	72k		0x011fff	4k		0x001000	phy_init
72k		0x012000	1.0M	0x0fffff	952k	0x0ee000	littlefs
1.0M	0x100000	2.5M	0x27ffff	1.5M	0x180000	ota 0
